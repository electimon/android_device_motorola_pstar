on early-init
   exec u:r:vendor_modprobe:s0 -- /vendor/bin/modprobe -a -d /vendor/lib/modules mmi_relay touchscreen_mmi stmicro_mmi wl2866d bq2597x_mmi mmi_parallel_charger mmi_parallel_charger_qc3p sx937x_sar st21nfc

on boot
# NFC
   chown nfc nfc /dev/st21nfc
   chmod 0660 /dev/st21nfc
# Touchscreen
   chown system system /sys/class/touchscreen/primary/interpolation
   chown system system /sys/devices/platform/soc/a94000.i2c/i2c-2/2-0049/double_click
